##
## CPU-arch: pentium3
##

feature-enable=platform/arch/x86/mmx:		on
feature-enable=platform/arch/x86/mmx2:		on
feature-enable=platform/arch/x86/sse:		on
feature-enable=platform/arch/x86/sse2:		off
feature-enable=platform/arch/x86/simd:		off	# mmx+3dnow together ?
feature-enable=platform/arch/x86/3dnow:		off
feature-enable=platform/arch/x86/3dnowex:	off

target-cputype:					pentium3
target-arch:					i686
